{
  "name": "state-manager-client",
  "version": "1.0.0",
  "main": "dist/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "@types/google-protobuf": "^3.15.6"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "grpc-tools": "^1.11.2",
    "grpc_tools_node_protoc_ts": "^5.3.2"
  },
  "scripts": {
    "clean": "rm -rf src/gen dist",
    "run": "ts-node src/index.ts",
    "codegen": "buf generate ../proto",
    "compile": "tsc --build",
    "copy-gen-files": "copyfiles -u 1 src/**/*.d.ts src/**/*.proto src/**/*.js dist/",
    "build": "yarn codegen && yarn compile && yarn copy-gen-files"
  }
}
